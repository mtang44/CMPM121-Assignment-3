using System.Collections.Generic;
public class ValueModifier {
    public List<string> mana_cost_mods;
    public List<Dictionary<string, int>> damage_mods;
    public List<int> cooldown_mod;
    public List<Dictionary<string, string>> projectile;

    void start()
    {

    }

    void update()
    {
    }
    // added this to value Modifier class from spell class
    public virtual void SetAttributes(JObject attributes)
    {
       
        name = attributes["name"].ToString();
        description = attributes["description"].ToString();
      
    }
}
